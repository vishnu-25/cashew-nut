<div class=" checkout-btn">
  <button type="button" class="btn btn" id="ck"  data-toggle="modal" data-target="#myModal">CHECK OUT</button>

  <!-- Modal -->
  <div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <form action="/details" method="post">
      <div class="modal-content">
        <div class="modal-header">
        
          <h4 class="modal-title text-center">ENTER YOUR DETAILS</h4>
          <button type="button" class="close" data-dismiss="modal">&times;</button>
        </div>
        <div class="modal-body">
          <input type="text" class="form-control mb-2" id="username" placeholder="enter your name" name="customerName">
          <input type="number" class="form-control mb-2 " id="userphone" placeholder="enter your ph number" name="customerPh">
          <textarea class="form-control" id="useraddress mb-2" rows="3" placeholder="Address" name="customerAddress"></textarea>
      
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-success" data-dismiss="modal">Buy</button>
          <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
        </div>
      </div>
      </form>
    </div>
  </div>
  
</div>

        <div class="clear"></div>
      </div>
